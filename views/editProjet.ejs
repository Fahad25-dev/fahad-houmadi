                    <div class="card"  style="padding:30px">
                        <h3 class="text-center">Formulaire d'ajout</h3>
                        <% if (locals.error != '') { %>
                            <div class="alert alert-danger" id="error"><%= locals.error %></div>
                        <% } %>
                        <form action="/projet/update" method="POST">
                            <input type="hidden" name="id"  id="id" value="<%= Projets.id %>">
                            <div class="form-group">
                                <label for="nom">Nom Projet</label>
                                <input type="text" name="nom_projet" id="nom_projet" value="<%= Projets. nom_projet %>" class="form-control">
                            </div>
                            <div class="form-group">
                                <label for="prenom">resource</label>
                                <input type="text" name="resource" id="resource" value="<%= Projets.resource %>" class="form-control">
                            </div>
                            <div class="form-group">
                                <label for="adresse">suivi</label>
                                <input type="text" name="suivi" id="suivi" value="<%= Projets.suivi %>" class="form-control">
                            </div>
                            <div class="form-group">
                                <label for="email">duree</label>
                                <input type="time" name="duree" id="duree" value="<%= Projets.duree %>" class="form-control">
                            </div>
                            <div class="d-flex justify-content-between"
                            style="margin-top: 10px">
                                <a href="/projet/list" class="btn btn-danger">Annuler</a>
                                <input type="submit" value="Modifier" class="btn btn-primary">
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
        <script>
            let error = '<%= locals.error %>'
            if(error != '') {
                setTimeout(() => {
                    document.getElementById('error').style.display = 'none'
